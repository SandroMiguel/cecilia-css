{
    "name": "cecilia-css",
    "version": "0.3.4",
    "description": "Cecilia CSS is a framework based on Bootstrap and Foundation: the best of both worlds.",
    "main": "dist/cecilia.min.css",
    "license": "MIT",
    "author": {
        "name": "Sandro Miguel Marques",
        "email": "sandromiguel@sandromiguel.com",
        "url": "http://sandromiguel.com"
    },
    "keywords": [
        "css",
        "sass",
        "framework",
        "responsive",
        "front-end"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/SandroMiguel/cecilia-css.git"
    },
    "bugs": {
        "url": "https://github.com/SandroMiguel/cecilia-css/issues"
    },
    "homepage": "http://kisscecilia.com",
    "scripts": {
        "build": "npx sass ./src/cecilia.scss ./dist/cecilia.css",
        "release": "standard-version"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "cz-conventional-changelog": "^3.2.0",
        "del": "^5.1.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-cssnano": "^2.1.3",
        "gulp-if": "^3.0.0",
        "gulp-imagemin": "^7.1.0",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-uglify": "^3.0.2",
        "husky": "^4.2.5",
        "normalize.css": "^8.0.1",
        "run-sequence": "^2.2.1",
        "standard-version": "^7.1.0"
    },
    "packageManager": "yarn@3.3.1"
}
